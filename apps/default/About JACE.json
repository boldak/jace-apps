> d.tag == \"swot_ua_arg_2020\" ?>)\n\n// ...\n\ndml.select(\n    from:\"sdi-threats\", \n    where:<? d => d.value.year == \"2017\" ?>,\n    return: <? \n        d => {\n            let res = {\n                \"3_alpha_code\": d.value[\"3_alpha_code\"],\n                \"country\": _.find($scope.countries, c => c[\"3_alpha_code\"] == d.value[\"3_alpha_code\"] ).long_name,\n                year: d.value.year\n            }\n            \n            _.pairs($scope.indicators).map( i => i[0]).forEach( a => {\n                res[a] = Number.parseFloat(Number.parseFloat(d.value[a]).toFixed(3))\n            })\n            return res\n          \n        }\n    ?>\n)\nset(\"data\")\n"},"id":"c7s6bnl7qkt"},{"type":"html-widget","name":"noname_clone_ghtwztr5wu_clone_mswe3ozpt8i","icon":"mdi-language-html5","options":{"widget":{"visible":true}},"data":{"source":"embedded","embedded":"<div class=\"title pa-2 mt-4 primary--text\" style=\"line-height:1em;\">\n  Collections\n</div>","script":""},"id":"mswe3ozpt8i"},{"type":"dps-highlight-widget","name":"noname","icon":"mdi-language-javascript","options":{"widget":{"visible":true}},"data":{"script":"// ...\n\nc.select(where:<? d => !Number.isNaN(d.ie) && !Number.isNaN(d.iec) && !Number.isNaN(d.is) ?>)\nset(\"data\")\nc.map(<? \n    d => _.keys($scope.indicators).map( i => d[i])\n?>)\nmatrix.transpose()\nset(\"table\")"},"id":"5xgh7r8daux"},{"type":"html-widget","name":"noname_clone_ghtwztr5wu_clone_mswe3ozpt8i_clone_homyxq6w9kb","icon":"mdi-language-html5","options":{"widget":{"visible":true}},"data":{"source":"embedded","embedded":"<div class=\"title pa-2 mt-4 primary--text\" style=\"line-height:1em;\">\n  Statistics\n</div>","script":""},"id":"homyxq6w9kb"},{"type":"dps-highlight-widget","name":"noname","icon":"mdi-language-javascript","options":{"widget":{"visible":true}},"data":{"script":"\nget(\"data\")\nc.map(<? \n    d => _.zipObject(_.drop(_.keys(d)), _.drop(_.values(d)))\n?>)\nc.map(<? \n    d => {\n        let res = {}\n        $scope.fields.forEach( f => {\n            res[f] = d[f] || 0\n        })\n        return res\n    }\n?>)\nset(\"pca_data\")\n\ns.pca.loadings()\nset(\"loadings\")\n\nget(\"pca_data\")\ns.pca.scores()\nset(\"scores\")\n\n\ns.cluster(\n    by:<? _.keys($scope.scores[0]).filter(k => k.startsWith(\"pc\"))?>,\n    count:6\n)\n\nc.map(<? \n   \n        (d,index) => ({\n          city: $scope.data[index].city,\n          cluster: d.cluster\n        })\n    \n?>)\nset(\"temp\")\n\n"},"id":"2tpqzm0g2ox"},{"type":"html-widget","name":"noname_clone_ghtwztr5wu_clone_mswe3ozpt8i_clone_homyxq6w9kb_clone_l2at72s4kqg","icon":"mdi-language-html5","options":{"widget":{"visible":true}},"data":{"source":"embedded","embedded":"<div class=\"title pa-2 mt-4 primary--text\" style=\"line-height:1em;\">\n  Services\n</div>","script":""},"id":"l2at72s4kqg"},{"type":"dps-highlight-widget","name":"noname","icon":"mdi-language-javascript","options":{"widget":{"visible":true}},"data":{"script":"service.es(\n    from: {{service}},\n    cmd:\"cluster.stats\"\n)\n\nset(\"data\")\n\n// ...\n\nservice.es(\n    from: {{service}},\n    cmd:\"indices.getMapping\",\n    args:<? {\n        index:$scope.index\n    } ?>\n    ,\n    returns:<? \n        d => {\n            let res = []\n            \n            let fetchFields = (properties, prefix) => {\n                _.keys( properties ).forEach( p => {\n                    let c = `${(prefix)?prefix+\".\":\"\"}${p}`\n                    res.push({\n                      id:c,\n                      title:c,\n                      data:{\n                          fieldName:c,    \n                          type: properties[p].type || \"object\"\n                      }\n                    })\n                })\n            }\n        \n            fetchFields(d[$scope.index].mappings.properties)\n            \n            return res\n        }\n    ?>\n)   "},"id":"44wzy8t9y7"}]},"r9f3w2dz7bd":{"widgets":[]}},"id":"dps","title":"dps","